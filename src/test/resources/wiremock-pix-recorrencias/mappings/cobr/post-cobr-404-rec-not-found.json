{
  "request": {
    "method": "POST",
    "url": "/cobr",
    "headers": {
      "Authorization": {
        "equalTo": "Bearer valid_token_with_cobr_write"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.idRec",
        "equalTo": "RR999999999999999999999999999"
      }
    ]
  },
  "response": {
    "status": 404,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "type": "https://pix.bcb.gov.br/api/v2/error/RecNaoEncontrada",
      "title": "Recorr\u00eancia n\u00e3o encontrada.",
      "status": 404,
      "detail": "N\u00e3o foi encontrada recorr\u00eancia para o idRec informado."
    }
  }
}