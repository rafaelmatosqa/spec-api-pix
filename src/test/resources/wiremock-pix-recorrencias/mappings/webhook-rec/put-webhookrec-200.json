{
  "request": {
    "method": "PUT",
    "url": "/webhookrec",
    "headers": {
      "Authorization": {
        "equalTo": "Bearer valid_token_with_webhookrec_write"
      },
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.webhookUrl",
        "contains": "https://"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "webhookUrl": "https://meusistema.com.br/webhook/pix-rec"
    }
  }
}